{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Gripmock",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/gripmock.go",
            "env": {
                "GRPC_PORT": "4773"
            },
            "args": [
            "--proto-dirs=/home/calin/90poe/buf",
            "--wkt-proto=/home/calin/dev/protobuf/src",]
        },
        {
            "name": "Connect to server",
            "type": "go",
            "request": "attach",
            "mode": "remote",
            "remotePath": "${workspaceFolder}/protoc-gen-gripmock/generator.go",
            "port": 42707,
            "host": "127.0.0.1",
            "apiVersion": 1
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/protoc-gen-gripmock/generator.go",
            "console": "externalTerminal"
        }
    ]
}
